<h1>Applications</h1>
 
<div class="row">
  <div class="col-md-8">
 <!-- #7 -->
    <% @applications.each do |application| %>
      <div class="media">
        <div class="media-body">
          <h4 class="media-heading">
 <!-- #8 -->
            <%= link_to application.name, application %>
          </h4>
          <small>
            Registered <%= time_ago_in_words(application.created_at) %> ago
          </small>
        </div>
      </div>
    <% end %>
  </div>

    <div class="col-md-4">
 <!-- #9 -->
      <%= link_to "New Application", new_registered_application_path, class: 'btn btn-success' %>
    </div>

</div>